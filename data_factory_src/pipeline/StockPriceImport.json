{
	"name": "StockPriceImport",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "CSVextendedIntraday",
						"type": "DatasetReference",
						"parameters": {
							"RelativeURL": {
								"value": "@concat('query?function=TIME_SERIES_INTRADAY_EXTENDED&symbol=', variables('SymbolV'), '&interval=', variables('IntervalV'), 'min&slice=year', variables('YearV'), 'month', variables('MonthV'), '&apikey=demo')",
								"type": "Expression"
							}
						}
					}
				]
			},
			{
				"name": "IncrementYear",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "YearV",
					"value": {
						"value": "@variables('IndexYear')",
						"type": "Expression"
					}
				}
			},
			{
				"name": "IncrementIndexYear",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "IndexYear",
					"value": {
						"value": "@string(add(int(variables('YearV')),1))",
						"type": "Expression"
					}
				}
			}
		],
		"variables": {
			"SymbolV": {
				"type": "String",
				"defaultValue": "IBM"
			},
			"IntervalV": {
				"type": "String",
				"defaultValue": "15"
			},
			"YearV": {
				"type": "String",
				"defaultValue": "1"
			},
			"MonthV": {
				"type": "String",
				"defaultValue": "1"
			},
			"IndexYear": {
				"type": "String",
				"defaultValue": "1"
			},
			"IndexMonth": {
				"type": "String",
				"defaultValue": "1"
			}
		},
		"annotations": []
	}
}